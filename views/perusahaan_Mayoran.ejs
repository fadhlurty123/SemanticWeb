<!DOCTYPE html>
<html>
<head>
  <title>Perusahaan Mayora</title>
  <link rel="stylesheet" type="text/css" href="stylesheets/style.css">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
  <div class="title">
    <div class="logo"><a href="/"><img src ="images/logo.png" width="80" height="80"/></a></div>
    <h1>Mayora Company</h1></div>
      <img class="company_logo"src ="https://assets.pikiran-rakyat.com/crop/0x0:0x0/x/photo/2020/09/09/4275572739.png" width="500" height="400"/>
    <div class="company_name">
      <h2>Product list :</h2></div>
  <div class="product-wrapper">
    <div class="product-list">
      <% makanan.forEach(function(makanan) { %>
        <div class="product-list-card">
          <h2 class="title-font"><%= makanan.nama %></h2>
          <p class="content-font">Varian Makanan : <%= makanan.varian  %> </p>
          <h3 class="content-font">Harga: <%= makanan.harga  %> </h3>
          <img class="content-font"src ="<%= makanan.urlFoto %>" width="400" height="400"/>

          
        </div>
      <% }); %> 
    </div>
  </div>
</body>
</html>